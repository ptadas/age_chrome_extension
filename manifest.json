{
  "manifest_version": 2,

  "name": "Age",
  "description": "This extension shows your age in 100 millisend time",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ],
  "chrome_url_overrides" : {
    "newtab": "myPage.html"
  }
}
